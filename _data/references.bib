@article{baker2018DoubledCliticsAre,
  title = {Doubled Clitics Are Pronouns: {{Amharic}} Objects (and Beyond)},
  shorttitle = {Doubled Clitics Are Pronouns},
  author = {Baker, Mark C. and Kramer, Ruth},
  year = {2018},
