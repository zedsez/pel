@misc{akkus2024CaseSyntaxArgument,
  title = {Case and the Syntax of Argument Indexation: {{An}} Analysis of   {{Sorani Kurdish}}},
  shorttitle = {Case and the Syntax of Argument Indexation},
  author = {Akkus, Faruk and Embick, David and Salih, Mohammed},
  year = {2024},
  month = feb,
  publisher = {LingBuzz},
  urldate = {2023-11-21},
  abstract = {This is the first draft of our book on case and the syntax of argument indexation.  In this book, we argue for an approach in which case labels ('Nominative', 'Ergative', etc.) are shorthand for bundles of decomposed features (cp. Halle and Vaux (1998), who do this for morphological realization), and maintain that the feature set is available not just in morphology but crucially in syntax as well. A key idea in the approach is what we call Case Targeting: the idea that probes may target arguments with specific case features. Importantly, syntax and morphology can target these features differentially, leading to various interesting mismatches. This includes allowing Clitic Movement in syntax to produce both morphophonological clitic and agreement affix on the PF side. Conversely, Agreement operation in syntax can produce morphophonological clitic and agreement affix on the PF side. We focus primarily on Sorani Kurdish varieties (Iranian languages), but also investigate and apply our approach to other Iranian languages (Kurmanji, Zazaki, Laki, Persian, Rushani, Shughni), Indo-Aryan (Hindi, Nepali, Gujarati, Maithili), Semitic (Arabic, Neo-Aramaic), and Polynesian (Nukuoro). Although we don't argue for a specific case assignment mechanism, we elaborate on various imports including for implicational hierarchies, case containment etc. We maintain that discussions of inherent vs dependent case approaches should be done at a finer granularity, one that is informed by the representation of case features that we argue for. Comments are very welcome!},
  archiveprefix = {LingBuzz},
  howpublished = {\url{https://ling.auf.net/lingbuzz/007378}},
  keywords = {affix,agreement,alignment,argument indexation,case,clitic,morphology,phonology,split-ergativity,syntax},
  annotation = {LingBuzz Published In: Submitted},
  file = {/Users/zedsez/Zotero/storage/K2FZ2HWQ/akkusetal_2024_case_and_the_syntax_of_argument_indexation.pdf;/Users/zedsez/Zotero/storage/3IICXFDD/007378.html}
