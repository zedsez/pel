@article{kalandadze2018FigurativeLanguageComprehension,
  title = {Figurative Language Comprehension in Individuals with Autism Spectrum Disorder: {{A}} Meta-Analytic Review},
  shorttitle = {Figurative Language Comprehension in Individuals with Autism Spectrum Disorder},
  author = {Kalandadze, Tamar and Norbury, Courtenay and N{\ae}rland, Terje and N{\ae}ss, Kari-Anne B},
