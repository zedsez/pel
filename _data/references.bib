@incollection{embick2023BlockingEffects,
  title = {Blocking {{Effects}}},
  booktitle = {The {{Wiley Blackwell Companion}} to {{Morphology}}},
  author = {Embick, David and Benz, Johanna and Paparounas, Lefteris},
  editor = {Ackema, Peter and Bendjaballah, Sabrina and Bonet, Eul{\`a}lia and F{\'a}bregas, Antonio},
  year = {2023},
