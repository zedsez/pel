@article{preminger2019WhatPCCTells,
  title = {What the {{PCC}} Tells Us about ``Abstract'' Agreement, Head Movement, and Locality},
  author = {Preminger, Omer},
  year = {2019},
  month = jan,
  journal = {Glossa: a journal of general linguistics},
  volume = {4},
  number = {1},
