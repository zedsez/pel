@article{deal2016SyntacticErgativityAnalysis,
  title = {Syntactic {{Ergativity}}: {{Analysis}} and {{Identification}}},
  shorttitle = {Syntactic {{Ergativity}}},
  author = {Deal, Amy Rose},
  year = {2016},
  month = jan,
  journal = {Annual Review of Linguistics},
