@article{harizanov2014CliticDoublingSyntaxmorphophonology,
  title = {Clitic Doubling at the Syntax-Morphophonology Interface: {{A-movement}} and Morphological Merger in {{Bulgarian}}},
  shorttitle = {Clitic Doubling at the Syntax-Morphophonology Interface},
  author = {Harizanov, Boris},
  year = {2014},
  month = nov,
  journal = {Natural Language \& Linguistic Theory},
  volume = {32},
